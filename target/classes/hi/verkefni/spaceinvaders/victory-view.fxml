<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="victoryimg" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hi.verkefni.spaceinvaders.GameOverController">
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
    <children>
        <ImageView fitHeight="610.0" fitWidth="607.0" layoutX="-6.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/victory.jpg" />
         </image></ImageView>
        <Button fx:id="playagain" layoutX="166.0" layoutY="500.0" mnemonicParsing="false" onMouseClicked="#playagain" prefHeight="25.0" prefWidth="270.0" style="-fx-background-color: #fcda00;" stylesheets="@styles.css" text="Play Again?" textAlignment="CENTER">
            <font>
                <Font name="Yu Gothic Light" size="26.0" />
            </font>
        </Button>
    </children>
</Pane>
